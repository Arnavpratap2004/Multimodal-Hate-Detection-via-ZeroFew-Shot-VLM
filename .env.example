# OpenRouter API Key (get from https://openrouter.ai/)
OPENROUTER_API_KEY=your_api_key_here

# Model Selection
# VLM options: openai/gpt-4o, google/gemini-pro-1.5, anthropic/claude-3-opus
VLM_MODEL=openai/gpt-4o

# LLM options: openai/gpt-4o, anthropic/claude-3-opus, meta-llama/llama-3.1-70b-instruct
LLM_MODEL=openai/gpt-4o

# Evaluation settings
DEFAULT_INFERENCE_MODE=zero_shot
MAX_RETRIES=3
REQUEST_TIMEOUT=60
